function getMe() {
  local id_file=${FACRI_OUTPUT}/me_id.txt
  local me_file=${FACRI_OUTPUT}/me.txt
  local me_id
  if [ -f $id_file ]; then
    me_id=$(cat $id_file) 
  else
    if [ ! -f $me_file ]; then 
      $FBCMD FINFO -id=1 friend_count,mutual_friend_count,pic_small,profile_url,sex,significant_other_id =me  > $me_file
    fi
    me_id=$(cat $me_file | grep -v UID | awk -F"," '{print $1}')
  fi
  echo $me_id
}

#getMe
